  # Create port connections
  connect_bd_net -net clk_wiz_0_clk_pix [get_bd_pins clk_wiz_0/clk_pix] [get_bd_pins v_axi4s_vid_out_0/vid_io_out_clk] [get_bd_pins v_tc_0/clk]
